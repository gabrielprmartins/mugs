{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "rules": {
        "import/prefer-default-export": 0,
        "no-use-before-define": 0,
        "no-param-reassign": 0,
        "object-curly-newline": 0,
        "import/extensions": 0,
        "no-plusplus": 0,
        "no-shadow": 0,
        "comma-spacing": 0,
        "prefer-const": 0
    }
}